package com.product;

public class LinkedList {
	private Node next;

	public LinkedList(Node next) {
		super();
		this.next = null;
	}

	public Node getNext() {
		return next;
	}

	public void setNext(Node next) {
		this.next = next;
	}
	
	public void insert(Product p) {
		Node product=new Node(p);
		Node head=product;
		if(head==null) {
			head=product;
			return;
		}
	}

}
